-- That file uses encoding UTF-8

CREATE TABLE [GDB_ColumnInfo]
 (
	[TableName]			Text (510) NOT NULL, 
	[FieldName]			Text (510) NOT NULL, 
	[columnFlags]			Long Integer NOT NULL
);

CREATE TABLE [GDB_DatabaseLocks]
 (
	[LockID]			Long Integer, 
	[LockType]			Long Integer NOT NULL, 
	[UserName]			Memo/Hyperlink (255) NOT NULL, 
	[MachineName]			Memo/Hyperlink (255) NOT NULL
);

CREATE TABLE [GDB_GeomColumns]
 (
	[TableName]			Text (510) NOT NULL, 
	[FieldName]			Text (510) NOT NULL, 
	[ShapeType]			Long Integer NOT NULL, 
	[ExtentLeft]			Double, 
	[ExtentBottom]			Double, 
	[ExtentRight]			Double, 
	[ExtentTop]			Double, 
	[IdxOriginX]			Double, 
	[IdxOriginY]			Double, 
	[IdxGridSize]			Double, 
	[SRID]			Long Integer NOT NULL, 
	[HasZ]			Boolean NOT NULL, 
	[HasM]			Boolean NOT NULL, 
	[ZLow]			Double, 
	[ZHigh]			Double, 
	[MLow]			Double, 
	[MHigh]			Double
);

CREATE TABLE [GDB_ItemRelationships]
 (
	[ObjectID]			Long Integer NOT NULL, 
	[UUID]			Replication ID NOT NULL, 
	[OriginID]			Replication ID NOT NULL, 
	[DestID]			Replication ID NOT NULL, 
	[Type]			Replication ID NOT NULL, 
	[Attributes]			Memo/Hyperlink (255), 
	[Properties]			Long Integer
);

CREATE TABLE [GDB_ItemRelationshipTypes]
 (
	[ObjectID]			Long Integer NOT NULL, 
	[UUID]			Replication ID NOT NULL, 
	[OrigItemTypeID]			Replication ID NOT NULL, 
	[DestItemTypeID]			Replication ID NOT NULL, 
	[Name]			Text (320) NOT NULL, 
	[ForwardLabel]			Text (510), 
	[BackwardLabel]			Text (510), 
	[IsContainment]			Integer
);

CREATE TABLE [GDB_Items]
 (
	[ObjectID]			Long Integer NOT NULL, 
	[UUID]			Replication ID NOT NULL, 
	[Type]			Replication ID NOT NULL, 
	[Name]			Text (320), 
	[PhysicalName]			Text (320), 
	[Path]			Memo/Hyperlink (255), 
	[DatasetSubtype1]			Long Integer, 
	[DatasetSubtype2]			Long Integer, 
	[DatasetInfo1]			Text (510), 
	[DatasetInfo2]			Text (510), 
	[URL]			Text (510), 
	[Definition]			Memo/Hyperlink (255), 
	[Documentation]			Memo/Hyperlink (255), 
	[ItemInfo]			Memo/Hyperlink (255), 
	[Properties]			Long Integer, 
	[Defaults]			OLE (255), 
	[Shape]			OLE (255)
);

CREATE TABLE [GDB_Items_Shape_Index]
 (
	[IndexedObjectId]			Long Integer NOT NULL, 
	[MinGX]			Long Integer NOT NULL, 
	[MinGY]			Long Integer NOT NULL, 
	[MaxGX]			Long Integer NOT NULL, 
	[MaxGY]			Long Integer NOT NULL
);

CREATE TABLE [GDB_ItemTypes]
 (
	[ObjectID]			Long Integer NOT NULL, 
	[UUID]			Replication ID NOT NULL, 
	[ParentTypeID]			Replication ID NOT NULL, 
	[Name]			Text (320) NOT NULL
);

CREATE TABLE [GDB_ReplicaLog]
 (
	[ID]			Long Integer NOT NULL, 
