	[FieldName]			Text (510) NOT NULL, 
	[ShapeType]			Long Integer NOT NULL, 
	[ExtentLeft]			Double, 
	[ExtentBottom]			Double, 
	[ExtentRight]			Double, 
	[ExtentTop]			Double, 
