	[SRTEXT]			Memo/Hyperlink (255) NOT NULL, 
	[FalseX]			Double, 
	[FalseY]			Double, 
	[XYUnits]			Double, 
	[FalseZ]			Double, 
	[ZUnits]			Double, 
	[FalseM]			Double, 
	[MUnits]			Double, 
	[IsHighPrecision]			Long Integer, 
	[XYTolerance]			Double, 
	[ZTolerance]			Double, 
	[MTolerance]			Double
);

CREATE TABLE [GDB_GeomColumns]
 (
